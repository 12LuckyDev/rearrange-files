{
  "name": "rearrange-files",
  "version": "0.1.0",
  "description": "",
  "keywords": [
    "files",
    "organize",
    "fs",
    "rearrange"
  ],
  "author": "12luckydev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/12LuckyDev/rearrange-files.git"
  },
  "source": "src/index.ts",
  "main": "lib/cjs/index.js",
  "types": "lib/cjs/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "test": "jest --config jest.config.js",
    "build": "npm-run-all build:clean build:cjs build:esm build:types",
    "build:clean": "rimraf lib",
    "build:cjs": "tsc -p tsconfig.cjs.json && echo {\"type\": \"commonjs\"} > lib/cjs/package.json",
    "build:esm": "tsc -p tsconfig.esm.json && echo {\"type\": \"module\"} > lib/esm/package.json",
    "build:types": "tsc -p tsconfig.types.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "dayjs": "^1.11.13",
    "exifreader": "^4.23.3",
    "fs-extra": "^11.2.0",
    "mime-types": "^2.1.35",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.8.4",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  }
}
